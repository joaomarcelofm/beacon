<%= simple_form_for current_user do |f| %>
  <%= f.input :name %>
  <%= f.input :location %>
  <%= f.input :occupation %>
  <%= f.input :bio %>
  <%= f.association :skills, collection: Skill.all, as: :check_boxes, multiple: true %>
  <%= f.association :interests, collection: Interest.all, as: :check_boxes, multiple: true %>
  <%= f.submit %>
<% end %>
